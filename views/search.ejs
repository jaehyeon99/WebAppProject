                            <% var { number, name, href, slideNum } = found[i] %>
                            <tr>
                            <div class="inner_cell">
                              <td><a href="<%= "https://selab.hanyang.ac.kr/courses/cse326/2019/" + href + "#slide" + slideNum %>"><div><%= number + "강 " + name + " 슬라이드 " + slideNum %></div></a></td>
                              <td class="date">2014.08.29</td>
                          </div>
                        </tr>
                          <% } %>
              </div>
          </table>
        </section>
      </div>
